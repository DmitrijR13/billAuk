    {
      Page1.Visible = false;
      Page2.Visible = false;
      
      string _nzp_type = (string)Report.GetParameterValue(&quot;nzp_type&quot;);
      
      if (_nzp_type == &quot;3&quot;) Page1.Visible = true;
