<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://cit.ru/gkhrt" version="1.0"
           xmlns:gkh="http://cit.ru/gkhrt" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:include schemaLocation="mab_core.xsd"/>

  <xs:complexType name="mabParameters">
    <xs:annotation>
      <xs:documentation>Общие сведения и техническое состояние МКД</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="nearHouseTerritoryAreaBySurfaceType" type="gkh:mabNearHouseTerritorySurfaceTypeArea">
        <xs:annotation>
          <xs:documentation>Площадь придомовой территории по типу покрытия</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="landArea" type="gkh:mabLandArea">
        <xs:annotation>
          <xs:documentation>Информация по площади земельного участка</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="technicalSummary" type="gkh:mabTechnicalSummary">
        <xs:annotation>
          <xs:documentation>Техническая информация</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="indoorArea" type="gkh:mabIndoorArea">
        <xs:annotation>
          <xs:documentation>Данные по внутренней площади</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="communityPropertyObjects" type="gkh:mabCommunityPropertyObject">
        <xs:annotation>
          <xs:documentation>Объекты общего имущества</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="communityPropertyRooms" type="gkh:mabCommunityPropertyRoom">
        <xs:annotation>
          <xs:documentation>Помещения, относящееся к общему долевому имуществу собственников помещений, кроме мест общего пользования</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="heatingEquipment" type="gkh:mabHeatingEquipment">
        <xs:annotation>
          <xs:documentation>Оборудование по отоплению, размещенное на внутридомовых инженерных системах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="coldWaterEquipment" type="gkh:mabColdWaterEquipment">
        <xs:annotation>
          <xs:documentation>Оборудование по холодному водоснабжению, размещенное на внутридомовых инженерных системах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hotWaterEquipment" type="gkh:mabHotWaterEquipment">
        <xs:annotation>
          <xs:documentation>Оборудование по горячему водоснабжению, размещенное на внутридомовых инженерных системах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="electricEquipment" type="gkh:mabElectricEquipment">
        <xs:annotation>
          <xs:documentation>Оборудование по электроснабжению, размещенное на внутридомовых инженерных системах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="elevators" type="gkh:mabElevator">
        <xs:annotation>
          <xs:documentation>Лифты</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="otherEquipment" type="gkh:mabOtherEquipment">
        <xs:annotation>
          <xs:documentation>Другое оборудование, размещенное на внутридомовых инженерных системах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="customEquipmentItems" type="gkh:mabCustomEquipment">
        <xs:annotation>
          <xs:documentation>Иное оборудование, размещенное на внутридомовых инженерных системах</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="premiseSummary" type="gkh:mabPremiseSummary">
        <xs:annotation>
          <xs:documentation>Общие сведения о помещениях</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="6" name="flatCountByOwnershipType" type="gkh:mabFlatCount">
        <xs:annotation>
          <xs:documentation>Количество квартир по типу собственности</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="energyEfficiency" type="gkh:mabEnergyEfficiency">
        <xs:annotation>
          <xs:documentation>Данные по энергоэффективности</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="maxEnergyConsumption" type="gkh:mabMaxEnergyConsumption">
        <xs:annotation>
          <xs:documentation>Характеристики максимального энергопотребления</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="energyConsumption" type="gkh:mabEnergyConsumption">
        <xs:annotation>
          <xs:documentation>Энергопотребление</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="mabAccrualParameters">
    <xs:annotation>
      <xs:documentation>Параметры МКД, влияющие на начисления</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="isTownTypeSettlement" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Тип населенного пункта (городской, сельский)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hasElectricRanges" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Наличие/отсутствие электроплит</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="coldWaterTypeWell" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Холодное водоснабжения типа скважина, колонка</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mabBathTubeType" type="gkh:mabBathTubeType">
        <xs:annotation>
          <xs:documentation>Наличие ванн</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mabWashbasinType" type="gkh:mabWashbasinType">
        <xs:annotation>
          <xs:documentation>Наличие умывальников</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="mabBathTubeType">
    <xs:annotation>
      <xs:documentation>Наличие ванн в МКД</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="NONE">
        <xs:annotation>
          <xs:documentation>Без ванн</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BATHTUB">
        <xs:annotation>
          <xs:documentation>Наличие ванн</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="WATER_HEATER">
        <xs:annotation>
          <xs:documentation>Наличие ванн и водонагревателей</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="WATER_HEATER_WITH_MULTI_POINT_PUMPING">
        <xs:annotation>
          <xs:documentation>Наличие ванн и водонагревателей с многоточечным водоразбором</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="mabWashbasinType">
    <xs:annotation>
      <xs:documentation>Наличие умывальников в МКД</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="NONE">
        <xs:annotation>
          <xs:documentation>Без умывальников</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="WASHBASIN">
        <xs:annotation>
          <xs:documentation>Наличие моек и умывальников</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SHOWER">
        <xs:annotation>
          <xs:documentation>Наличие моек и умывальников и душа</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SITTING_BATHTUB">
        <xs:annotation>
          <xs:documentation>Наличие душа и сидячих ванн</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FULL_SCALE_BATHTUB">
        <xs:annotation>
          <xs:documentation>Наличие душа и ванн с размерами 1500-1700</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="mabHeatingType">
    <xs:annotation>
      <xs:documentation>Тип отопления в МКД</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="CENTRAL">
        <xs:annotation>
          <xs:documentation>Центральное</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AUTONOMOUS_BOILER_ROOM">
        <xs:annotation>
          <xs:documentation>Автономная котельная (крышная, встроено-пристроенная)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FLAT_BOILER">
        <xs:annotation>
          <xs:documentation>Квартирное отопление (квартирный котел)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="STOVE_HEATING">
        <xs:annotation>
          <xs:documentation>Печное</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABSENT">
        <xs:annotation>
          <xs:documentation>Отсутствует</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="mabColdWaterSupplyType">
    <xs:annotation>
      <xs:documentation>Тип холодного водоснабжения в МКД</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="CENTRAL">
        <xs:annotation>
          <xs:documentation>Центральное</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABSENT">
        <xs:annotation>
          <xs:documentation>Отсутствует</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="mabHotWaterSupplyType">
    <xs:annotation>
      <xs:documentation>Тип горячего водоснабжения в МКД</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="CENTRAL_OPEN">
        <xs:annotation>
          <xs:documentation>Центральное - открытая система</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CENTRAL_CLOSED">
        <xs:annotation>
          <xs:documentation>Центральное - закрытая система</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AUTONOMOUS_BOILER_ROOM">
        <xs:annotation>
          <xs:documentation>Автономная котельная (крышная, встроено-пристроенная)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FLAT_BOILER">
        <xs:annotation>
          <xs:documentation>Квартирное отопление (индивидуальный котел)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="INDIVIDUAL_WATER_HEATER">
        <xs:annotation>
          <xs:documentation>Квартирное отопление (индивидуальный котел)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="STOVE_WATER_HEATER">
        <xs:annotation>
          <xs:documentation>От дровяных колонок</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABSENT">
        <xs:annotation>
          <xs:documentation>Отсутствует</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="mabElectricSupplyType">
    <xs:annotation>
      <xs:documentation>Тип электроснабжения в МКД</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="CENTRAL">
        <xs:annotation>
          <xs:documentation>Центральное</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABSENT">
        <xs:annotation>
          <xs:documentation>Отсутствует</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="mabGasSupplyType">
    <xs:annotation>
      <xs:documentation>Тип газоснабжения в МКД</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="CENTRAL">
        <xs:annotation>
          <xs:documentation>Центральное</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NONCENTRAL">
        <xs:annotation>
          <xs:documentation>Нецентральное</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABSENT">
        <xs:annotation>
          <xs:documentation>Отсутствует</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="mabCanalizationType">
    <xs:annotation>
      <xs:documentation>Тип канализации в МКД</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="CENTRAL">
        <xs:annotation>
          <xs:documentation>Центральная</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABSENT">
        <xs:annotation>
          <xs:documentation>Отсутствует</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="elevatorShaftType">
    <xs:annotation>
      <xs:documentation>Тип шахты лифта</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="ATTACHED">
        <xs:annotation>
          <xs:documentation>Приставная</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BUILTIN">
        <xs:annotation>
          <xs:documentation>Встроенная</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="rubbishChuteReceptionLoadingValvePlacement">
    <xs:annotation>
      <xs:documentation>Размещение приемо-загрузочных клапанов мусоропроводов</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="FLAT">
        <xs:annotation>
          <xs:documentation>В квартирах</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="STAIRCASE">
        <xs:annotation>
          <xs:documentation>На лестничных клетках</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="STAIRCASE_SEPARATE_ROOM">
        <xs:annotation>
          <xs:documentation>В обособленных помещениях на лестничной клетке</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="gutterType">
    <xs:annotation>
      <xs:documentation>Тип водостоков</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="OUTSIDE">
        <xs:annotation>
          <xs:documentation>Наружные</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="INSIDE">
        <xs:annotation>
          <xs:documentation>Внутренние</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABSENT">
        <xs:annotation>
          <xs:documentation>Отсутствуют</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ventilationType">
    <xs:annotation>
      <xs:documentation>Тип вентиляции</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="INLET">
        <xs:annotation>
          <xs:documentation>Приточная</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="EXHAUST">
        <xs:annotation>
          <xs:documentation>Вытяжная</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="INLET_EXHAUST">
        <xs:annotation>
          <xs:documentation>Приточно-вытяжная</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABSENT">
        <xs:annotation>
          <xs:documentation>Отсутствует</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="energyEfficiencyClass">
    <xs:annotation>
      <xs:documentation>Класс энергоэффективности</xs:documentation>
    </xs:annotation>
    <xs:restriction base="gkh:string">
      <xs:enumeration value="A">
        <xs:annotation>
          <xs:documentation>Наивысший (A)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BPP">
        <xs:annotation>
          <xs:documentation>Повышенный (B++)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BP">
        <xs:annotation>
          <xs:documentation>Повышенный (B+)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="B">
        <xs:annotation>
          <xs:documentation>Высокий (B)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="C">
        <xs:annotation>
          <xs:documentation>Нормальный (C)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="D">
        <xs:annotation>
          <xs:documentation>Пониженный (D)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="E">
        <xs:annotation>
          <xs:documentation>Низший (E)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="mabNearHouseTerritorySurfaceTypeArea">
    <xs:annotation>
      <xs:documentation>Площадь придомовой территории МКД по типу покрытия</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="improved" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Усовершенствованное/неусовершенствованное покрытие</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type">
        <xs:annotation>
          <xs:documentation>Тип покрытия</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="gkh:string">
            <xs:maxLength value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="area" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь, м2</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="mabLandArea">
    <xs:annotation>
      <xs:documentation>Информация по площади земельного участка многоквартирного дома</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="landActualUse" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Общая площадь земельного участка по фактическому пользованию, всего (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="landActualUseBuiltUp" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Общая площадь земельного участка по фактическому пользованию, застроенная, всего (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="landActualUseNotBuiltUp" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Общая площадь земельного участка по фактическому пользованию, незастроенная, всего (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hardSurface" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь твердых покрытий (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hardSurfacePassages" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь проездов (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hardSurfaceSidewalks" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь тротуаров (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hardSurfaceOther" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь объектов с твердым покрытием, кроме прездов и тротуаров (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="playgrounds" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь детских площадок (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sportsGrounds" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь спортивных площадок (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="otherGrounds" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь других площадок, кроме детских и спортивных (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="greenSpacesTotal" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь зеленых насаждений, всего (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="parks" type="gkh:area">
        <xs:annotation>
          <xs:documentation>Площадь скверов (м2)</xs:documentation>
        </xs:annotation>
      </xs:element>
