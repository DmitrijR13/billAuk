        TbS.Font =  Text13.Font;
        TbS.Format=  Text13.Format;
        TbS.Border=Text13.Border;
        TbS.Text= &quot;[isum&quot;+i+&quot;]&quot;;
        TbS.Name = &quot;TbS&quot;+i;
        DataFooter1.Objects.Add(TbS);
      }
      
    }

